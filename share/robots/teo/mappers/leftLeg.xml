<?xml version="1.0" encoding="UTF-8"?>

<device name="leftLegMapper" type="controlboardremapper">
    <param name="AxesNames">
        (AxialLeftHip SagittalLeftHip FrontalLeftHip FrontalLeftKnee FrontalLeftAnkle SagittalLeftAnkle)
    </param>
    <action phase="startup" level="5" type="attach">
        <paramlist name="networks">
            <elem name="AxialLeftHip">ID7</elem>
            <elem name="SagittalLeftHip">ID8</elem>
            <elem name="FrontalLeftHip">ID9</elem>
            <elem name="FrontalLeftKnee">ID10</elem>
            <elem name="FrontalLeftAnkle">ID11</elem>
            <elem name="SagittalLeftAnkle">ID12</elem>
        </paramlist>
    </action>
    <action phase="shutdown" level="20" type="detach" />
</device>
