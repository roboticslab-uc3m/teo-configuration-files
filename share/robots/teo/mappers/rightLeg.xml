<?xml version="1.0" encoding="UTF-8"?>

<device name="rightLegMapper" type="controlboardremapper">

    <param name="axesNames">
        (AxialRightHip
         SagittalRightHip
         FrontalRightHip
         FrontalRightKnee
         FrontalRightAnkle
         SagittalRightAnkle)
    </param>

    <action phase="startup" level="15" type="attach">
